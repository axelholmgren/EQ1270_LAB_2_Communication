%% load
kth = load('kth.jpg');

%% encode pic prep 1 
pic = imread('kth.jpg');

[key,cPic] = encoder(pic);

%% Make some fucking noise! prep 2
h = [1 0.7 0.7];
dist_key = conv(key, h,'same');


%% decode pic prep 3
dPic = decoder(dist_key, cPic);
axis square;
image(dPic);

%% HHHH
dsist_key=[];
for n = 1:length(key)
    
    conv_vector = [key()]
    conv_result = conv(h,conv_vector)
    dsist_key= [dsist_key, conv_result]
end
